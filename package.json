{
  "name": "moviedb",
  "description": "",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "dev": "NODE_ENV=development npm start",
    "bundle": "webpack --config webpack.config.prod.js",
    "clean": "rimraf ./dist",
    "release": "npm install && npm run clean && npm run bundle && NODE_ENV=production npm start",
    "build": "npm run clean && npm run bundle",
    "server": "cd .. && cd moviedb-back && node app.js",
    "server-dev": "cd .. && cd moviedb-back && npm run dev-server",
    "db": "cd .. && cd moviedb-back && mongod --dbpath db",
    "all": "concurrently --kill-others \"npm run db\" \"npm run server\" \"npm run release\"",
    "all-dev": "concurrently --kill-others \"npm run db\" \"npm run server-dev\" \"npm run dev\""
  },
